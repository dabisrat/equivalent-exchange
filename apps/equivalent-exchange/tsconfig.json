{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@eq-ex/app/*": [
        "./src/*"
      ],
      "@eq-ex/ui": [
        "../../packages/ui/src/index.ts"
      ],
      "@eq-ex/ui/*": [
        "../../packages/ui/src/*"
      ],
      "@eq-ex/auth": [
        "../../packages/auth/index.ts"
      ],
      "@eq-ex/auth/*": [
        "../../packages/auth/*"
      ],
      "@eq-ex/shared": [
        "../../packages/shared/index.ts"
      ],
      "@eq-ex/shared/*": [
        "../../packages/shared/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}